{
  "name": "datetime-helper",
  "type": "module",
  "packageManager": "pnpm@10.10.0",
  "version": "0.0.1",
  "scripts": {
    "generate:iana-timezones": "node scripts/generate-iana-timezones.mjs",
    "dev": "astro dev",
    "prebuild": "pnpm generate:iana-timezones",
    "build": "astro build",
    "build:web-component": "vite build --lib src/web-component/datetime-helper-element.ts --formats es --fileName datetime-helper-element",
    "build:wasm": "cd crates/datetime-engine && wasm-pack build --target web",
    "pretest": "pnpm generate:iana-timezones",
    "test": "vitest run",
    "test:e2e": "playwright test",
    "preview": "astro preview",
    "astro": "astro"
  },
  "devDependencies": {
    "@astrojs/svelte": "^7.2.5",
    "@playwright/test": "^1.58.2",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/svelte": "^5.3.1",
    "@vvo/tzdb": "^6.198.0",
    "astro": "^5.17.1",
    "jsdom": "^28.1.0",
    "svelte": "^5.51.1",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}